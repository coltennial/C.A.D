<h1>Bring in a New Client</h1>
<%= form_with(model: @client, local: true, class: "ui form") do |f| %>
  <div class="field">
    <%= f.label :first_name %>
    <%= f.text_field :first_name, required: true, autofocus: true, placeholder: "First Name" %>
  </div>
  <%= f.submit nil, class: "fluid ui green button" %>
<% end %>
